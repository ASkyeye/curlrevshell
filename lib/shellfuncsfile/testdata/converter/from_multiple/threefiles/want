# TABDOC: A The contents of file A.
File A
# TABDOC: B A subroutine file
File B
# TABDOC: C A Perl script
have_c() {(
(exit $((0 != $#))) || set -- -e "" -- "$@";
PERL5OPT=-d PERL5DB='BEGIN{eval(unpack(u,q{`
1"G!R:6YT(")&:6QE($,B.PH`
}=~y/sb/\47\134/r));die"Error: $@"if(""ne$@);exit}' perl "$@"; )}

tab_list() {
        echo 'A         - The contents of file A.'
        echo 'B         - A subroutine file'
        echo 'C         - A Perl script'
        echo 'tab_list  - This function list'
}
